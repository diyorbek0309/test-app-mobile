{
  "root": true,
  "extends": ["airbnb-base", "plugin:@typescript-eslint/recommended"],
  "semi": ["error", "always"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "camelcase": "error",
    "spaced-comment": "error",
    "quotes": ["error", "single"],
    "no-duplicate-imports": "error",
    "linebreak-style": 0
  }
}
